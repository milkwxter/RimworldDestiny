<Defs>

	<!--=============== RISKRUNNA ====================-->
    <SoundDef>
        <defName>Milkwater_Shot_Riskrunner</defName>
        <context>MapOnly</context>
        <eventNames/>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <grains>
                    <li Class="AudioGrain_Clip">
                        <clipPath>Weapons/shot_riskrunner</clipPath>
                    </li>
                </grains>
                <volumeRange>
                    <min>35</min>
                    <max>40</max>
                </volumeRange>
                <pitchRange>
                    <min>1.2</min>
                    <max>1.25</max>
                </pitchRange>
                <sustainLoop>False</sustainLoop>
            </li>
        </subSounds>
    </SoundDef>
	
